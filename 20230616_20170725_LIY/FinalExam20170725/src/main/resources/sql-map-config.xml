<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>	
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="question" type="com.myApp.biz.question.QuestionVO" />
		<typeAlias alias="member" type="com.myApp.biz.member.MemberVO" />
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/question-mapping.xml" />
		<mapper resource="mappings/member-mapping.xml" />
	</mappers>
</configuration>